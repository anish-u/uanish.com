{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "es5",
  "importOrder": [
    "^(react?/?([a-zA-Z/]*))$",
    "^(react|next?/?([a-zA-Z/]*))$",
    "<THIRD_PARTY_MODULES>",
    "^@/components/(.*)$",
    "^@/contexts/(.*)$",
    "^@/services/(.*)$",
    "^@/utils/(.*)$",
    "^@/interfaces/(.*)$",
    "^@/store/(.*)$",
    "^@/env/(.*)$",
    "^@/constants/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
